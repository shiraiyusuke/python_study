<script type="text/javascript">
var rest=5;
var redirectFlg=true;
var coundDownTimer=0;

function countDown(){
	if(redirectFlg){
		writeCount();
		if(rest<=0){
			location.href="http://www.nifty.com/?top20_c403";
			clearTimeout(coundDownTimer);
			rest=5;
		}
		rest-=1;
		coundDownTimer=setTimeout("countDown();",1000);
	}
}

function writeCount(){
	if(document.getElementById){
		document.getElementById("count").innerHTML=rest;
	} else if(document.all){
		document.all("count").innerHTML=rest;
	}
}

function checkbox(){
	if(document.forms["stopform"].elements["stopcheck"].checked){
		redirectFlg=false;
		clearTimeout(coundDownTimer);
	}else{
		redirectFlg=true;
		countDown();
	}
}

function reset(){
	rest=5;
}
</script>


